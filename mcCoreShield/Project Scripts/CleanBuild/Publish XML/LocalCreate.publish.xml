<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <IncludeCompositeObjects>True</IncludeCompositeObjects>
    <TargetDatabaseName>mcCoreShield</TargetDatabaseName>
    <DeployScriptFileName>mcCoreShield.sql</DeployScriptFileName>
    <TargetConnectionString>Data Source=CA-UNUA;Integrated Security=True;Pooling=False</TargetConnectionString>
    <ProfileVersionNumber>1</ProfileVersionNumber>
    <ScriptDatabaseOptions>True</ScriptDatabaseOptions>
    <BlockOnPossibleDataLoss>True</BlockOnPossibleDataLoss>
    <CreateNewDatabase>False</CreateNewDatabase>
    <IgnoreAnsiNulls>False</IgnoreAnsiNulls>
    <DoNotAlterChangeDataCaptureObjects>False</DoNotAlterChangeDataCaptureObjects>
    <DoNotAlterReplicatedObjects>False</DoNotAlterReplicatedObjects>
    <DropConstraintsNotInSource>False</DropConstraintsNotInSource>
    <DropDmlTriggersNotInSource>False</DropDmlTriggersNotInSource>
    <DropExtendedPropertiesNotInSource>False</DropExtendedPropertiesNotInSource>
    <DropIndexesNotInSource>False</DropIndexesNotInSource>
    <IgnoreFillFactor>False</IgnoreFillFactor>
    <IgnoreIdentitySeed>True</IgnoreIdentitySeed>
    <IgnoreIncrement>True</IgnoreIncrement>
    <IgnoreIndexPadding>False</IgnoreIndexPadding>
    <IgnoreKeywordCasing>False</IgnoreKeywordCasing>
    <IgnoreObjectPlacementOnPartitionScheme>False</IgnoreObjectPlacementOnPartitionScheme>
    <IgnorePermissions>True</IgnorePermissions>
    <IgnoreQuotedIdentifiers>False</IgnoreQuotedIdentifiers>
    <IgnoreRoleMembership>True</IgnoreRoleMembership>
    <IgnoreRouteLifetime>False</IgnoreRouteLifetime>
    <IgnoreWithNocheckOnCheckConstraints>False</IgnoreWithNocheckOnCheckConstraints>
    <PopulateFilesOnFileGroups>False</PopulateFilesOnFileGroups>
    <ScriptDeployStateChecks>True</ScriptDeployStateChecks>
    <BackupDatabaseBeforeChanges>True</BackupDatabaseBeforeChanges>
    <GenerateSmartDefaults>True</GenerateSmartDefaults>
  </PropertyGroup>
  <ItemGroup>
    <SqlCmdVariable Include="PostDeployChanges">
      <Value>NO</Value>
    </SqlCmdVariable>
    <SqlCmdVariable Include="PreDeployChanges">
      <Value>NO</Value>
    </SqlCmdVariable>
  </ItemGroup>
</Project>